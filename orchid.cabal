Name:             orchid
Version:          0.0.10
Description:      Haskell Wiki Library
Synopsis:         Haskell Wiki Library
Category:         Network, Web
Cabal-version:    >= 1.6
License:          BSD3
License-file:     LICENSE
Author:           Sebastiaan Visser
Maintainer:       sfvisser@cs.uu.nl
Build-Type:       Simple
Build-Depends:    base,
                  bytestring,
                  salvia-protocol,
                  salvia,
                  salvia-extras,
                  orchid-doc,
                  xml,
                  extensible-exceptions,
                  containers,
                  parsec,
                  fclabels,
                  filestore,
                  monads-fd,
                  transformers,
                  encoding,
                  time,
                  stm,
                  process,
                  unix,
                  nano-md5,
